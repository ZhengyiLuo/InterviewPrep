import java.util.PriorityQueue;

/**
 * @Time: Nov 5, 2016
 * @Author: zen
 */

public class kthElement {
    public int find(int [] array, int k) {
        PriorityQueue<Integer> pq = new PriorityQueue<>(k);
        for (int i = 0; i < array.length; i++) {
            int element = array[i];
            if (pq.isEmpty()) {
              pq.add(element);  
            }
            
        }
        
        
        
        
        
        return 0;
    }

}
